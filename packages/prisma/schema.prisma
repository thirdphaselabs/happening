generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id        String   @id @default(cuid())
  title     String   @unique
  createdAt DateTime @default(now())
}

model Waitlist {
  id        Int   @id @default(autoincrement())
  email     String   @unique
  createdAt DateTime @default(now())
}